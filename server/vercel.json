{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "api/*": {
      "runtime": "@vercel/node@1.10.0", 
      "memory": 512,
      "maxDuration": 20
    }
  }
}
